# Production environment variables
NODE_ENV=production
PORT=8080

# Data file paths
LEADS_FILE=server/data/prod/leads.json
SCHEDULER_FILE=server/data/prod/scheduler.json

# Rate limiting
RATE_LIMIT_MAX_REQUESTS=100

# Authentication
ADMIN_USERNAME=admin
# Use a strong password or provide a hash
# ADMIN_PASSWORD=your-production-password
# ADMIN_PASSWORD_HASH=

# External services
OPENAI_API_KEY=your-openai-key
TWILIO_SID=your-twilio-sid
TWILIO_AUTH=your-twilio-auth
TWILIO_PHONE_DEV=
TWILIO_PHONE_PROD=+15555555555
SERVER_BASE_URL=https://your-production-url
