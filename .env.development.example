# Development environment variables
NODE_ENV=development
PORT=3000

# Data file paths
LEADS_FILE=server/data/dev/leads.json
SCHEDULER_FILE=server/data/dev/scheduler.json

# Rate limiting
RATE_LIMIT_MAX_REQUESTS=100

# Authentication
ADMIN_USERNAME=admin
ADMIN_PASSWORD=devpassword
# ADMIN_PASSWORD_HASH can be used instead of ADMIN_PASSWORD
# ADMIN_PASSWORD_HASH=

# External services
OPENAI_API_KEY=your-openai-key
TWILIO_SID=your-twilio-sid
TWILIO_AUTH=your-twilio-auth
TWILIO_PHONE_DEV=+15555555555
# Set production number separately to avoid cross-environment usage
TWILIO_PHONE_PROD=
SERVER_BASE_URL=http://localhost:3000
